!!! 5
html
  head
    title ride2go
    meta(http-equiv='content-type', content='text/html; charset=iso-8859-1')
    script(src='http://maps.google.com/maps/api/js?sensor=false')
    script(src='/js/jquery.js')
    script(src='/js/jquery.ui.js')
    script(src='/socket.io/socket.io.js')
    script(src='/js/ride2go.js')
    script(src='/js/autocomplete.js')
              
    link(type='text/css', href='/css/smoothness/jquery-ui-1.8.14.custom.css', rel='stylesheet')
    link(type='text/css', href='/css/main.css', rel='stylesheet')
    
    style(type='text/css')
                  .ui-autocomplete { overflow-y: auto; width:300px; } * html .ui-autocomplete { /*
                  IE max- */height: expression( this.scrollHeight > 320 ? "320px" : "auto" ); } .ui-autocomplete
                  { max-height: 320px; } .ui-autocomplete li { font-size:10pt; }
              
  body
    #header
      span#logo ride2go
      span#status status: disconnected 
      span#mode
        ul
          li.splash.mode_active splash
          li.to to
          li.from from
          li.result result 
      span#social 
        span#gplus gplus
        span#facebook gplus

    #left
      #from_panel
        h2 departing
        h3.cityname Berlin
        img(src='/staticmap.berlin.png')
        a#edit_from_input edit
        #depature_startdate 18/11/2011 9:00 am
        #depature_endate 18/11/2011 7:21 pm 

    #right
      #to_panel
        h2 arriving
        h3.cityname Berlin
        img(src='/staticmap.hamburg.png')
        a#edit_to_input edit
        #depature_startdate 18/11/2011 9:00 am
        #depature_endate 18/11/2011 7:21 pm 

 
     
    #middle 
      #options bahn? bus? mfg?
      #rides
        ul 

    #middle_overlay
      #inputbox
      
      #to_input
        h1 Where to?
        #to_input_field_wrapper
          input#to_input_field(type='text')
      #from_input
        h1 From where?
        input#from_input_field(type='text')
 
